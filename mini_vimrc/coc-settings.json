{
  "explorer.sources": [
    {
      "name": "buffer",
      "expand": false
    },
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.keyMappings.global": {
    "<space>": "toggleSelection"
  },
  "explorer.presets": {
    "floating": {
      "position": "floating",
      "open-action-strategy": "sourceWindow"
    }
  },
  "explorer.icon.enableNerdfont": true,
  "explorer.buffer.tabOnly": true,
  "explorer.file.revealWhenOpen": false,
  "explorer.debug": true
}
