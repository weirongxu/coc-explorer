{
  "rulesDirectory": ["src"],
  "rules": {
    "semicolon": true,
    "trailing-comma": [true, "all"],
    "quotemark": [true, "single", "avoid-escape"],
    "curly": true,
    "prefer-const": true,
    "no-floating-promises": true,
    "ban": [true, ["workspace", "showMessage"], ["log"], ["prettyPrint"]]
  }
}
